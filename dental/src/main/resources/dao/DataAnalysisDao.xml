<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ricky.dentalclinic.dental.dao.DataAnalysisDao">
    <select id="countTotalCase" resultType="java.lang.Integer">
        SELECT COUNT(*)
        FROM
        t_case
        <where>
            <if test="year != null and year != ''">
                AND date LIKE concat(#{year},"%")
            </if>
        </where>
    </select>
    <select id="countPerMonth" resultType="java.lang.Integer">
        SELECT COUNT(*)
        FROM
        t_case
        <where>
            <if test="month != null and month != ''">
                AND date LIKE concat(#{month},"%")
            </if>
        </where>
    </select>
    <select id="getAllBirthday" resultType="java.util.Date">
        SELECT birthday
        FROM
        t_case
        <where>
            <if test="year != null and year != ''">
                AND date LIKE concat(#{year},"%")
            </if>
        </where>
    </select>
</mapper>